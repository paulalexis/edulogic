void draw_component(int n) {
  display.clearDisplay();  // Clear the display buffer

  if (n >= 0 && n <= 5) {
    display.drawBitmap(0, 0, bitmaps[n], 128, 40, WHITE);
  } else if (n == 6) {
    if ((x + y + z + unit_index) % 2 == 1) {
      display.drawBitmap(0, 0, epd_bitmap_int_on, 128, 40, WHITE);
    } else {
      display.drawBitmap(0, 0, epd_bitmap_int_off, 128, 40, WHITE);
    }
  }

  int x0 = 36;

  display.setCursor(x0, 50);
  display.setTextColor(WHITE);  // Set text color
  display.setTextSize(2);       // Set text size

  if ((component == 3) or (component == 4) or (component == 6)){
    ;
  } else {
    // Display the value
    display.print(x);
    display.print(y);
    display.print(z);
    
    // Draw a line under the currently selected digit/unit
    int line_y = 50-2;
    int size_one_digit = 10;
    if (selected == 0) {
      display.drawLine(x0, line_y-1, x0 + size_one_digit-1, line_y-1, WHITE);  // Under 'x'
      display.drawLine(x0, line_y, x0 + size_one_digit-1, line_y, WHITE);  // Under 'x'
    } else if (selected == 1) {
      display.drawLine(x0 + size_one_digit + 2, line_y-1, x0 + 2*size_one_digit + 1, line_y-1, WHITE);  // Under 'y'
      display.drawLine(x0 + size_one_digit + 2, line_y, x0 + 2*size_one_digit + 1, line_y, WHITE);  // Under 'y'
    } else if (selected == 2) {
      display.drawLine(x0 + 2*size_one_digit + 4, line_y-1, x0 + 3*size_one_digit + 3, line_y-1, WHITE);  // Under 'z'
      display.drawLine(x0 + 2*size_one_digit + 4, line_y, x0 + 3*size_one_digit + 3, line_y, WHITE);  // Under 'z'
    } else if (selected == 3) {
      display.drawLine(x0 + 3*size_one_digit + 6, line_y-1, x0 + 4*size_one_digit + 5, line_y-1, WHITE); // Under '?'
      display.drawLine(x0 + 3*size_one_digit + 6, line_y, x0 + 4*size_one_digit + 5, line_y, WHITE); // Under '?'
    }

    // Display the unit (Ohm, kOhm, MOhm)
    display.print(" ");
    if (unit_index == 1){
      display.drawBitmap(x0 + 3*size_one_digit + 6, 50, epd_bitmap_micro, 10, 14, WHITE);  // Omega (Ohm)
    } else {
      display.setCursor(x0 + 3*size_one_digit + 6, 50);
      display.print(power[unit_index]);
    }

    if (component == 0){
      display.drawBitmap(x0 + 4*size_one_digit + 8, 50, epd_bitmap_omega, 10, 14, WHITE);  // Omega (Ohm)
    } else {
      display.setCursor(x0 + 4*size_one_digit + 8, 50);
      display.print(units[component]);
    }
  }

  // display.setTextSize(1); 
  // display.setTextColor(WHITE); 
  // display.setCursor(100, 56);

  // char hexString[5];
  // sprintf(hexString, "0x%02X", SLAVE_ADDR);
  // display.println(hexString);

  display.display();  // Refresh the display
}

void setup() {
  Serial.begin(9600);
  sw.begin();
  if (!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  } else {
    Serial.println(F("SSD1306 allocation success"));
  }
  millis_button_buffer = millis();


  pinMode(buttonIncr, INPUT_PULLUP);
  pinMode(buttonBar, INPUT_PULLUP);
  pinMode(buttonComponent, INPUT_PULLUP);

  pinMode(RED_PIN, OUTPUT);
  pinMode(GREEN_PIN, OUTPUT);
  pinMode(BLUE_PIN, OUTPUT);

  // //////////////// I2C
  Wire.begin(SLAVE_ADDR);  // Initialize as I2C slave
  Wire.onReceive(receiveEvent);  // Register receive event
  Wire.onRequest(requestEvent);
  // Set all pins as INPUT to avoid floating states
  for (int i = 0; i < 4; i++) {
      pinMode(pins[i], INPUT);
  }

  // ///////////////////

  draw_component(component);
}

void loop() {
  int state_buttonIncr = 1 - int(analogRead(buttonIncr) > 10);  // Read buttonIncr (select)
  int state_buttonBar = 1 - int(analogRead(buttonBar) > 10);
  int state_buttonComponent =  1 - digitalRead(buttonComponent);  // Read buttonBar (increase)

  // Serial.print(state_buttonIncr);Serial.print(";");Serial.print(state_buttonBar);Serial.print(";");Serial.println(state_buttonComponent);
  
  // Handle the selection cycle with buttonIncr
  if (state_buttonIncr == HIGH && millis() - millis_button_buffer > buffer_time) {
    selected = (selected + 1) % 4;  // Cycle through x, y, z, ?
    millis_button_buffer = millis();  // Update the debounce time
  }

  // Handle value increment with buttonBar
  if (state_buttonBar == HIGH && millis() - millis_button_buffer > buffer_time) {
    if (selected == 0) {
      x = (x + 1) % 10;  // Increment 'x' and reset if over 9
    } else if (selected == 1) {
      y = (y+1)%10;  // Increment 'y' and reset if over 9
    } else if (selected == 2) {
      z = (z+1)%10;  // Increment 'z' and reset if over 9
    } else if (selected == 3) {
      unit_index = (unit_index + 1) % 7;  
    }
    millis_button_buffer = millis();  // Update the debounce time
  }

  if (state_buttonComponent == HIGH && millis() - millis_button_buffer > buffer_time) {
    component = (component + 1) % 7; 
    millis_button_buffer = millis();  // Update the debounce time
  }

  if (millis_button_buffer == millis()){
    draw_component(component);
  }

  if (millis() > time_led + 750){
    // digitalWrite(RED_PIN, 0);
    // digitalWrite(GREEN_PIN, 0);
    digitalWrite(BLUE_PIN, 0);
  }
}


// /////////////////////////// I2C

// Function to handle received I2C data
void receiveEvent(int numBytes) {
    // Check if we received exactly 3 bytes
    if (numBytes != 3) {
        Serial.println("Type: Request.");
        int requestTypeEph = Wire.read();
        if (requestTypeEph != 0){
          requestType = requestTypeEph;
        }
        Serial.print("Number : "); Serial.println(requestType);
        Serial.print("Nb Bytes : "); Serial.println(numBytes);
        return;
    } else {
      requestType = 0;
    }

    // Read the 3 bytes
    byte port = Wire.read();
    byte protocol_id = Wire.read();
    byte received_data = Wire.read();

    // Handle protocol_id 0: Set pin mode to INPUT or OUTPUT
    if (protocol_id == 0) {
        pinMode(pins[port], received_data == 1 ? INPUT : OUTPUT);
        Serial.print("Port ");
        Serial.print(port);
        Serial.print(" set as ");
        Serial.println(received_data == 1 ? "INPUT" : "OUTPUT");
    }
    // Handle protocol_id 1: Set pin to HIGH or LOW
    else if (protocol_id == 1) {
        if (received_data == 1){
          digitalWrite(BLUE_PIN, 0);
          digitalWrite(RED_PIN, 1);
        } else {
          digitalWrite(RED_PIN, 0);
        }
        pinMode(pins[port], OUTPUT);
        digitalWrite(pins[port], received_data == 1 ? HIGH : LOW);
        Serial.print("Port ");
        Serial.print(port);
        Serial.print(" set as ");
        Serial.println(received_data == 1 ? "HIGH" : "LOW");
    }
}

byte binaryPinsState(bool &anyHigh) {
    byte pinState = 0x00;  // Initialize pinState (all LOW)
    anyHigh = false;        // Default to false
    for (int i = 0; i < 4; i++) {
        if (digitalRead(pins[i]) == HIGH) {
            pinState |= (1 << i);  // Set the i-th bit if the pin is HIGH
            anyHigh = true;        // At least one pin is HIGH
        }
    }
    return pinState;
}


void requestEvent(){
    // Serial.print("Protocol: "); Serial.println(requestType);
    Serial.println("*********************");
    
    if (requestType == 0x10) {
        Wire.write(component);  
        Wire.write(x);
        Wire.write(y);
        Wire.write(z);
        Wire.write(unit_index);
        Serial.print("Sending: ");
        Serial.print(component); Serial.print(", ");
        Serial.print(x); Serial.print(", ");
        Serial.print(y); Serial.print(", ");
        Serial.print(z); Serial.print(", ");
        Serial.println(unit_index);
        digitalWrite(BLUE_PIN, 1);
        time_led = millis();
    } else if (requestType == 0x11) {
        bool isAnyHigh = false;
        byte pinState = binaryPinsState(isAnyHigh);
        Wire.write(pinState);
        Serial.print("Sending pin states: ");
        Serial.println(pinState, BIN);
        if (isAnyHigh){
          digitalWrite(BLUE_PIN, 1);
        }
        time_led = millis();
    }

    // Read the 3 bytes
    byte port = Wire.read();
    byte protocol_id = Wire.read();
    byte received_data = Wire.read();

    // Handle protocol_id 0: Set pin mode to INPUT or OUTPUT
    if (protocol_id == 0) {
        pinMode(pins[port], received_data == 1 ? INPUT : OUTPUT);
        Serial.print("Port ");
        Serial.print(port);
        Serial.print(" set as ");
        Serial.println(received_data == 1 ? "INPUT" : "OUTPUT");
    }
    // Handle protocol_id 1: Set pin to HIGH or LOW
    else if (protocol_id == 1) {
        pinMode(pins[port], OUTPUT);
        digitalWrite(pins[port], received_data == 1 ? HIGH : LOW);
        Serial.print("Port ");
        Serial.print(port);
        Serial.print(" set as ");
        Serial.println(received_data == 1 ? "HIGH" : "LOW");
  }
}

